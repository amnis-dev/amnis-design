    "baseUrl": "./",
    "paths": {
      "@amnis/hooks/*": ["./packages/amnis-hooks/src/*"],
      "@amnis/style/*": ["./packages/amnis-style/src/*"],
      "@amnis/icons/*": ["./packages/amnis-icons/src/*"],
      "@amnis/display/*": ["./packages/amnis-display/src/*"],
      "@amnis/layout/*": ["./packages/amnis-layout/src/*"],
      "@amnis/entry/*": ["./packages/amnis-entry/src/*"],
      "@amnis/lands/*": ["./packages/amnis-lands/src/*"],
    }
  },
  "include": ["./packages/**/*.ts*"]
}